package com.learning.simplecore;

public class Product {
	
	private int price;		// primitive field property
	private Laptop laptop;	// object field property
	
	public Product() {
		System.out.println("Product: Default constructor is invoked.");
	}

	public Product(int price, Laptop laptop) {
		super();
		this.price = price;
		this.laptop = laptop;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}

	public Laptop getLaptop() {
		return laptop;
	}

	public void setLaptop(Laptop laptop) {
		this.laptop = laptop;
	}

	public void code() {
		laptop.compile();		
	}
}
